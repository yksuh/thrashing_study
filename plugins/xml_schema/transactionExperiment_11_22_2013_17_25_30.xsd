<?xml version="1.0"?>
<!-- Schema for Configurations -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="transactionExperiment">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="description" type="xsd:string" minOccurs="1" maxOccurs="1" />
				<xsd:choice minOccurs="1" maxOccurs="1">
					<xsd:element name="dataDefinitionReference">
						<xsd:complexType>
							<xsd:attribute name="href" type="xsd:string" use="required" />
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="dataDefinition"></xsd:element>
				</xsd:choice>
				<xsd:choice minOccurs="1" maxOccurs="1">
					<xsd:element name="tableConfigurationReference">
						<xsd:complexType>
							<xsd:attribute name="href" type="xsd:string" use="required" />
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="tableConfiguration">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="tableSet" minOccurs="0"
									maxOccurs="1">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="table" minOccurs="1" maxOccurs="unbounded">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="cardinality" minOccurs="1"
															maxOccurs="1">
															<xsd:complexType>
																<xsd:attribute name="hypothetical" use="required">
																	<xsd:simpleType>
																		<xsd:union>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:enumeration value="actual" />
																				</xsd:restriction>
																			</xsd:simpleType>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:positiveInteger" />
																			</xsd:simpleType>
																		</xsd:union>
																	</xsd:simpleType>
																</xsd:attribute>
															</xsd:complexType>
														</xsd:element>
													</xsd:sequence>
													<xsd:attribute name="name" type="xsd:string"
														use="required" />
													<xsd:attribute name="seed" type="xsd:string"
														use="required" />
												</xsd:complexType>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:choice>
				<xsd:element name="multiProcessorConfiguration" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute name="min" type="xsd:string" use="optional" />
						<xsd:attribute name="max" type="xsd:string" use="optional" />
						<xsd:attribute name="increment" type="xsd:string" use="optional" />
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="readSel" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute name="min" type="xsd:string" use="required" />
						<xsd:attribute name="max" type="xsd:string" use="required" />
						<xsd:attribute name="increment" type="xsd:string" use="required" />
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="updateSel" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute name="min" type="xsd:string" use="required" />
						<xsd:attribute name="max" type="xsd:string" use="required" />
						<xsd:attribute name="increment" type="xsd:string" use="required" />
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="multiProgrammingLevel" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute name="min" type="xsd:string" use="required" />
						<xsd:attribute name="max" type="xsd:string" use="required" />
						<xsd:attribute name="increment" type="xsd:string" use="required" />
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="rowSubsetSize" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:attribute name="min" type="xsd:string" use="required" />
						<xsd:attribute name="max" type="xsd:string" use="required" />
						<xsd:attribute name="increment" type="xsd:string" use="required" />
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="name" type="xsd:string" use="required" />
			<xsd:attribute name="scenario" type="xsd:string" use="required" />
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
