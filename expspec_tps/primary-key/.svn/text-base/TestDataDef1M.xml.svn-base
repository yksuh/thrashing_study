		<column name="id1" dataType="number" dataLength="20" dataGenerationType="sequential" distributionMinimum="0" distributionMaximum="1000000" inPrimaryKey="true"/>

		<column name="id2" dataType="number" dataLength="20" dataGenerationType="random" distributionMinimum="0" distributionMaximum="1000000" inPrimaryKey="false"/>

		<column name="id3" dataType="number" dataLength="20" dataGenerationType="random" distributionMinimum="0" distributionMaximum="1000000" inPrimaryKey="false"/>

