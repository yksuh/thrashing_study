package azdblab.observer.api;

/**
 * An interface that allows a listener to layout a graph in 
 * the GUI.
 * @author Kevan Holdaway
 *
 */
public interface ResultGraphLayoutInterface {
	/**
	 * Use the moving layout to layout the graph.
	 */
	public void layoutMoving();
	/**
	 * Use the traditional layout to layout the graph.
	 */
	public void layoutTraditional();
}
