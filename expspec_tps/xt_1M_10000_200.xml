<transactionExperiment name="xt-1M-10000-200" scenario="xactthrashing">
  <description>
    The baseline test of transaction thrashing scenario.
  </description>
  <dataDefinitionReference href="pkTestDataDef1M.xml"/>
  <tableConfiguration>
    <tableSet>
      <table name="HT1" seed="1999">
        <cardinality hypothetical="actual"/>
      </table>
      <table name="HT2" seed="2999">
        <cardinality hypothetical="actual"/>
      </table>
    </tableSet>
  </tableConfiguration>
  <multiCoreConfiguration numberCores="6" min="0" max="8" increment="2"/>
  <readSel min="0.0001" max="0.1" increment="10"/><!-- selectivity in (%)-->
  <updateSel min="0.0001" max="0.1" increment="10"/> <!-- exclusive lock ratio -->
  <multiProgrammingLevel min="100" max="1000" increment="100"/>
  <rowSubsetSize min="1" max="1" increment="1"/>
</transactionExperiment>
