package azdblab.observer.api;

/**
 * Used to allow the listener to close an experiment.
 * @author Kevan Holdaway
 *
 */
public interface ResultCloseInterface {
	/**
	 *  Closes an experiment from the GUI.
	 */
	public void close();
	/**
	 * Closes all experiments from the GUI.
	 */
	public void closeAll();
}
